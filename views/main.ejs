<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Students</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">  
</head>
<body>
    <div class="container" style="background-image: url(https://images.unsplash.com/photo-1578852612716-854e527abf2e?q=80&w=1374&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D);">
        <div class="container mt-5 mb-5 d-flex flex-column w-100 justify-content-between align-items-center ">
            <h3 class="mr-2">Students_List</h3>
            <div class="container-fluid d-flex flex-column w-25 mt-2 ">
                <button class="btn btn-primary ">
                    <a  style="text-decoration: none; color: white;" href="/List_of_Interviews">List of Interviews</a>
                </button>
                <button class="btn btn-primary mt-2  ">
                    <a  style="text-decoration: none; color: white;" href="/AddNewListPage">Add New Student</a>
                </button>
              
            </div>
        </div>
        <div class="table-responsive">
            <table class="table table-bordered table-hover">
                <thead class="thead-dark">
                    <tr>
                        <th>S.no</th>
                        <th>Student_Name</th>
                        <th>College_Name</th>
                        <th>College_Status</th>
                        <th>DSA_Final_Score</th>
                        <th>WebD_final_Score</th>
                        <th>React_Final_Score</th>
                        <th>Schedule an Interview</th>
                    </tr>
                </thead>
                <tbody>
                    <% Student_Data.forEach(function(data, index) { %>
                        <tr>
                            <td class="bg-black text-white "><%= index + 1 %></td>
                            <td class="bg-black text-white " ><%= data.Student_Name %></td>
                            <td class="bg-black text-white "><%= data.Student_Details.College || 'N/A' %></td>
                            <td class="bg-black text-white "><%= data.Student_Details.Status || 'N/A' %></td>
                            <td class="bg-black text-white "><%= data.Course_Score.DSA_Final_Score || 'N/A' %></td>
                            <td class="bg-black text-white "><%= data.Course_Score.WebD_final_Score || 'N/A' %></td>
                            <td class="bg-black text-white "><%= data.Course_Score.React_Final_Score || 'N/A' %></td>
                            <td class="bg-black text-white "><a href="/Schedule_an_Interview_/<%= data._id %>">Schedule an Interview</a></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
        
    </div>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
</body>
</html>
